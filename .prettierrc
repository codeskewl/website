{
  "semi": true,
  "htmlWhitespaceSensitivity": "strict",
  "endOfLine": "lf",
  "proseWrap": "always",
  "printWidth": 80,
  "htmlSingleAttributePerLine": true,
  "importOrder": [
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "next-seo.config",
    "^components/(.*)$",
    "^utils/(.*)$",
    "^assets/(.*)$",
    "^@fontsource/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
